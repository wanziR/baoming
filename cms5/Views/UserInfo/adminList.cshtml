@model Models.UserInfo

@{
    ViewBag.Title = "adminList";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<div class="container-widget">

    <div class="row">

        <!-- Start Top Stats -->
        <div class="col-md-12">
            <ul class="topstats clearfix">
                <li class="arrow"></li>
                <li class="col-xs-6 col-lg-2">
                    <span class="title"><i class="fa fa-clock-o"></i> 新的缴费</span>
                    <h3 class="color-down">5!</h3>

                </li>
                <li class="col-xs-6 col-lg-2">
                    <span class="title"><i class="fa fa-dot-circle-o"></i> 今日报名人数</span>
                    <h3>10</h3>

                </li>
                <li class="col-xs-6 col-lg-2">
                    <span class="title"><i class="fa fa-calendar-o"></i> 今日缴费人数</span>
                    <h3>5</h3>

                </li>
                <li class="col-xs-6 col-lg-2">
                    <span class="title"> <i class="fa fa-users"></i> 报名总人数</span>
                    <h3 class="color-up">155</h3>

                </li>
                <li class="col-xs-6 col-lg-2">
                    <span class="title"><i class="fa fa-shopping-cart"></i>已缴费总人数</span>
                    <h3>112</h3>

                </li>


            </ul>
        </div>

        <!-- End Top Stats -->
        <div class="col-md-12">
            <div class="panel panel-default">
                <div class="panel-title">
                    报名人员信息
                </div>
                <div class="panel-body table-responsive">

                    <table id="example" class="table display">
                        <thead>
                            <tr>
                                <th>姓名</th>
                                <th>电话</th>
                                <th>公司</th>
                                <th>是否缴费</th>
                                <th>报名时间</th>
                                <th>详细信息</th>
                            </tr>
                        </thead>



                        <tbody>
                            @if (ViewBag.List != null)
                            {
                                foreach (var alist in ViewBag.list)
                                {
                                    <tr>
                                        <td>@alist.userName</td>
                                        <td>@alist.userPhone</td>
                                        <td>@alist.userCompany</td>
                                        <td><a href="#"><span style="color: red">@alist.taxIsok</span></td>
                                        <td>@alist.userAddtime</td>
                                        <td><a href="#">详情</a></td>
                                    </tr>
                                }
                            }
                        </tbody>

                    </table>


                </div>

            </div>
        </div>
    </div>
    </div>